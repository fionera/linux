menu "Realtek platform dependent drivers"

config RTK_KDRIVER_SUPPORT
    bool "RTK288O platform dependent drivers"
    default n
    depends on ARCH_RTK288O
    select RTK_KDRIVER
    ---help---

config RTK_KDRIVER
    bool
	depends on RTK_KDRIVER_SUPPORT
	default n

